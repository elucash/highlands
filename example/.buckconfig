[alias]


# BUCK will not look inside these for build configs
[project]
	ignore = \
		.out, \
		.gen, \
		.git, \
		.settings

[java]
	source_level = 1.8
	target_level = 1.8
	src_roots = /src, /lib/immutables/src
#	skip_checking_missing_deps = true

[log]
	max_traces = 1
	compress_traces = true

[buildfile]
	includes = //lib/DEFS
